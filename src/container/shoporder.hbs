<main class="p-4">
    {{#with navigation}}
        <div class="row">
            {{#each links}}
                <div class="col-2">
                    <div class="nav nav-tabs">
                        <div class="nav-item">
                            <a href="{{href}}" class="nav-link">{{text}}</a>
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>
    {{/with}}
    <div class="row">
        <div class="col-8">
            <div class="mt-4">
                <h1>{{header}}</h1>
            </div>
            <div class="mt-5">
            {{#with contacts_data}}
                <h2 class="mb-3">{{title}}</h2>
                {{#each buttons}}
                    {{#if isPrimary}}<button class="btn btn-warning">{{text}}</button>{{/if}}
                    {{#if isOutline}}<button class="btn btn-outline-warning">{{text}}</button>{{/if}}
                {{/each}}
                <div class="row">
                    {{#with form}}
                        {{#with input}}
                            {{#with surname}}
                                <div class="mt-2 col-6">
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control" placeholder="{{placeholder}}">
                                    <div class="form-text">{{caption}}</div>
                                </div>
                            {{/with}}
                            {{#with name}}
                                <div class="mt-2 col-6">
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control" placeholder="{{placeholder}}">
                                    <div class="form-text">{{caption}}</div>
                                </div>
                            {{/with}}
                            {{#with phone}}
                                <div class="mt-2 col-6">
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control" placeholder="{{placeholder}}">
                                    <div class="form-text">{{caption}}</div>
                                </div>
                            {{/with}}
                            {{#with email}}
                                <div class="mt-2 col-6">
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control" placeholder="{{placeholder}}">
                                    <div class="form-text">{{caption}}</div>
                                </div>
                            {{/with}}
                        {{/with}}
                        <hr class="m-4">
                        <div>
                            {{#each switch}}
                                <div class="form-check form-switch">
                                    <label class="form-check-label">{{text}}</label>
                                    <input type="checkbox" class="form-check-input">
                                </div>
                            {{/each}}
                        </div>
                        <hr class="m-4">
                    {{/with}}
                </div>
            {{/with}}
            <div class="row">
                {{#with delivery}}
                    <h2>{{title}}</h2>
                    {{#each select}}
                    <div class="card col-6">
                        <label class="form-label">{{label}}</label>
                        <select class="form-select">
                            <option selected>{{placeholder}}</option>
                            {{#each options}}
                            <option value="{{value}}">{{text}}</option>
                            {{/each}}
                        </select>
                    </div>
                    {{/each}}
                    <div class="mt-3">
                        {{#each delivery_type}}
                            <div class="form-check">
                                <label class="form-check-label">{{type}}</label>
                                <input type="radio" class="form-check-input">
                            </div>
                        {{/each}}
                    </div>
                    <hr class="m-4">
                    <div>
                        {{#with payment}}
                            <h2>{{title}}</h2>
                            {{#each list}}
                                <div class="form-check">
                                    <label class="form-check-label">{{type}}</label>
                                    <input type="radio" class="form-check-input">
                                    {{#if description}}<div class="form-text">{{description}}</div>{{/if}}
                                </div>
                            {{/each}}
                        {{/with}}
                    </div>
                    <hr class="m-4">
                    <div>
                        {{#with contact}}
                            <h2>{{title}}</h2>
                            <div class="row">
                                {{#with surname}}
                                    <div class="mt-2 col-6">
                                        <label class="form-label">{{label}}</label>
                                        <input class="form-control" placeholder="{{placeholder}}">
                                        <div class="form-text">{{caption}}</div>
                                    </div>
                                {{/with}}
                                {{#with name}}
                                    <div class="mt-2 col-6">
                                        <label class="form-label">{{label}}</label>
                                        <input class="form-control" placeholder="{{placeholder}}">
                                        <div class="form-text">{{caption}}</div>
                                    </div>
                                {{/with}}
                                {{#with phone}}
                                    <div class="mt-2 col-6">
                                        <label class="form-label">{{label}}</label>
                                        <input class="form-control" placeholder="{{placeholder}}">
                                        <div class="form-text">{{caption}}</div>
                                    </div>
                                {{/with}}
                                {{#with email}}
                                    <div class="mt-2 col-6">
                                        <label class="form-label">{{label}}</label>
                                        <input class="form-control" placeholder="{{placeholder}}">
                                        <div class="form-text">{{caption}}</div>
                                    </div>
                                {{/with}}
                            </div>
                        {{/with}}
                    </div>
                    <hr class="m-4">
                    <div>
                        {{#with coment}}
                            <div>
                                <label class="form-label">{{label}}</label>
                                <input class="form-control" placeholder="{{placeholder}}">
                            </div>
                        {{/with}}
                    </div>
                    <hr class="m-4">
                {{/with}}
            </div>
            <div>
                {{#with coupon}}
                    <h2>{{title}}</h2>
                    <div class="m-3">
                        {{#with select}}
                            {{#with availabel_coupon}}
                                <div>
                                    <label class="form-label">{{label}}</label>
                                    <div>
                                        <select class="form-select-lg">
                                            <option selected>{{placeholder}}</option>
                                            {{#each options}}
                                            <option value="{{value}}">{{text}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                </div>
                            {{/with}}
                            {{#with friend_coupon}}
                                <div>
                                    <label class="form-label">{{label}}</label>
                                    <div>
                                        <select class="form-select-sm">
                                            <option selected>{{placeholder}}</option>
                                            {{#each options}}
                                            <option value="{{value}}">{{text}}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                </div>
                            {{/with}}
                        {{/with}}
                        {{#with add}}
                            {{#with friend}}
                                <div>
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control" placeholder="{{placeholder}}">
                                    <div class="form-text">{{caption}}</div>
                                </div>
                            {{/with}}
                            {{#with colleague}}
                                <div>
                                    <label class="form-label">{{label}}</label>
                                    <input class="form-control" placeholder="{{placeholder}}">
                                    <div class="form-text">{{caption}}</div>
                                </div>
                            {{/with}}
                        {{/with}}
                    </div>
                {{/with}}
            </div>
            <hr class="m-4">
            <div>
                {{#with quiz}}
                    <h2 class="mb-3">{{title}}</h2>
                    <div>
                        {{#with preference}}
                            <p class="mb-3">{{title}}</p>
                            {{#each options}}
                            <div class="mb-3">
                                <input type="radio" class="form-check-input">
                                <label class="form-check-label">{{this}}</label>
                            </div>
                            {{/each}}
                        {{/with}}
                        {{#with your_expirience}}
                            <div>
                                <label class="form-label">{{label}}</label>
                                <input class="form-control">
                            </div>
                        {{/with}}
                        {{#with purchase_frequency}}
                            <label class="form-label">{{label}}</label>
                            <div>
                                <select class="form-select-sm">
                                    <option selected>{{placeholder}}</option>
                                    {{#each options}}
                                    <option value="{{value}}">{{text}}</option>
                                    {{/each}}
                                </select>
                            </div>
                        {{/with}}
                        {{#with factors}}
                            <div class="m-4">{{title}}</div>
                            <div class="form-check">
                                {{#each options}}
                                <label class="form-label">{{this}}</label>
                                <input type="checkbox" class="form-check-input">
                                {{/each}}
                            </div>
                        {{/with}}
                    </div>
                {{/with}}
            </div>
        </div>
        </div>
        <div class="col-4">
             <div class="card p-2">
                {{#each action_block}}
                    {{#if isSuccess}}<button class="btn btn-success m-2">{{text}}</button>{{/if}}
                    {{#if isDanger}}<button class="btn btn-danger m-2">{{text}}</button>{{/if}}
                    {{#if isInfo}}<button class="btn btn-info m-2">{{text}}</button>{{/if}}
                {{/each}}
            </div>
            <div class="card mt-3">
                {{#with total}}
                    <h3 class="p-1">{{title}}</h3>
                    <div>
                        {{#each item_list}}
                        <ul>
                            <li>{{name}}: {{price}}</li>
                        </ul>
                        {{/each}}
                        <hr>
                        {{#each additional_info}}
                        <ul>
                            <li>{{text}}: {{value}}</li>
                        </ul>
                        {{/each}}
                        <hr>
                        <div class="m-2">
                        {{#with total_price}}
                            {{text}}: {{value}}
                        {{/with}}
                        </div>
                        <div class="ms-2">
                            {{#each button}}
                                {{#if isPrimary}}<button class="btn btn-primary">{{text}}</button>{{/if}}
                                {{#if isOutline}}<button class="btn btn-outline-primary">{{text}}</button>{{/if}}
                            {{/each}}
                        </div>
                    </div>
                {{/with}}
            </div>
        </div>
    </div>
    <hr class="m-4">
    <div>
        <div class="row mb-5">
        {{#each footer}}
            <ul class="nav flex-column col-2">
                {{#each this}}
                {{#if @last}}
                    <li class="nav-link">
                        <a class="nav-link disabled" href="{{link}}">{{text}}</a>
                    </li>
                    {{else}}
                    <li class="nav-link">
                        <a class="nav-link" href="{{link}}">{{text}}</a>
                    </li>
                {{/if}}
                {{/each}}
            </ul>
        {{/each}}
        </div>
    </div>
</main>